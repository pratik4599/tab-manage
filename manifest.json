{
  "manifest_version": 3,
  "name": "Tab Stack Manager",
  "version": "1.0",
  "description": "Organize tabs into multiple vertical stacks",
  "permissions": [
    "tabs",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
